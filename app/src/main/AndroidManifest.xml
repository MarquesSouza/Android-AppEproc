<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.marquesdesouza.appeproc">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/brasaotocantins"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".LoginActivity"
            android:screenOrientation="portrait" />
        <activity
            android:screenOrientation="portrait"
            android:name=".MeusProcessoActivity"
            android:label="@string/title_activity_meus_processo"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:screenOrientation="portrait"
            android:name=".ConsultaActivity"
            android:label="@string/title_activity_consulta"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".ProcessoActivity"
            android:screenOrientation="portrait"
            ></activity>
    </application>

</manifest>